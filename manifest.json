{
  "manifest_version": 2,

  "name": "notes",
  "description": "Creating the notes with popup element",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "css": ["fp.css"],
      "js": ["fp.js"]
    }
  ],
  "browser_action": {
    "default_icon": "notepad.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": ["edit.png"]
}